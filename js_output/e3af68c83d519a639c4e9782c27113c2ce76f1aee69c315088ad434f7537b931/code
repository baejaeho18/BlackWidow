jQuery(document).ready(function(){function e(e){for(var s=JSON.parse(e),t="",r="",l="",n=0;n<4;n++)r=s[n].Url,l=s[n].title,t=s[n].PublishedDate,l.length>45&&(l=l.substring(0,45)+"..."),t=t.substring(0,t.indexOf(" ")),document.querySelector(o+n+" .vscom-devblogs-link").setAttribute("data-bi-name",s[n].Author+" "+t),document.querySelector(o+n+" .vscom-devblogs-link").href=r,document.querySelector(o+n+" .vscom-devblogs-slide-title").innerHTML=l,document.querySelector(o+n+" .vscom-devblogs-slide-excerpt").innerHTML=s[n].summary.substring(0,135)+"...",document.querySelector(o+n+" .vscom-devblogs-slide-authorname").innerHTML=s[n].Author,document.querySelector(o+n+" .vscom-devblogs-slide-authoricon").src=s[n].AuthorImage,document.querySelector(o+n+" .vscom-devblogs-slide-authoricon").alt="photo of "+s[n].Author,document.querySelector(o+n+" .vscom-devblogs-slide-postdate").innerHTML=t,jQuery(o+n).attr("data-title",l),jQuery('.vscom-slider--modern-devblogs-posts li[aria-posinset="'+(n+1)+'"] .vscom-slider-jump-link').attr("aria-label",l),jQuery('.vscom-slider--modern-devblogs-posts li[aria-posinset="'+(n+1)+'"] .vscom-slider-jump-link').attr("data-link-title",l),jQuery('.vscom-slider--modern-devblogs-posts li[aria-posinset="'+(n+1)+'"] .vscom-slider-jump-link').attr("data-bi-compnm","slider"),jQuery('.vscom-slider--modern-devblogs-posts li[aria-posinset="'+(n+1)+'"] .vscom-slider-jump-link').attr("data-bi-name","slider jump link"+(n+1))}var o=".devblogs-post.vscom-slide-",s=new XMLHttpRequest;s.open("GET","/wp-content/plugins/vscom-utility-plugin/json/home-devblogs.json"),s.setRequestHeader("Content-Type","application/json"),s.onreadystatechange=function(){4===s.readyState&&(200===s.status?e(s.response):console.log(s))},s.send()});